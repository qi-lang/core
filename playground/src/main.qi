# contains a module named IO
require qi.io

module A do
  def hello do
    Rand.between(0, 200)
  end
end

def __main__ do
  val = A.hello()
  cond do
    (val > 100) and (val < 50) ->
      IO.puts("Damn. The value of #{val} is big!")
    val > 100 ->
      IO.puts("Wow Big number #{val}!")
    _ ->
      IO.puts("Meh that's a boring number...")
  end
end
